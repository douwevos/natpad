module {

	name 'gross-dung';
	
	dependencies {
	}

	clear {
		remove '[%=module.builddir%]';
	}
	
	compile {
		javac { 
			src { path './src/java/'; }
			destdir './compiled/';
		}
	}

	package {
	
		jar { 
			in { 
				path './compiled/';
				exclude '.*Task.class';
				exclude '.*Action.class';
				exclude '.*Session.class';
			}
			destfile './gross-dung.jar';
		}
	}	


	export {
		lib {
			path './gross-dung.jar';
		}
	}


}